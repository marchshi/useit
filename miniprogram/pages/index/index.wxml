<!--index.wxml-->
<view class="container">
  <view class="bg-search">
    <navigator class="index-search" url="/pages/search/search" hover-class="navigator-hover" open-type="navigate">
      搜索企业
    </navigator>
  </view>
  <view class="swiper-view">
    <swiper indicator-dots="{{indicatorDots}}"
        autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
        <block wx:for="{{background}}" wx:key="*this">
          <swiper-item>
            <image class="index-image" src="../../images/{{item}}" data-id="{{index}}" mode="widthFix" />
          </swiper-item>
        </block>
      </swiper>
  </view>
  

  <view>
    <text class="" selectable="false" space="false" decode="false">
      我收藏的企业
    </text>
  </view>

  <view>
    <view>
      <view>
        <text class="" selectable="false" space="false" decode="false">
          江苏天力电缆有限公司
        </text>
      </view>
      <view>
        <text class="" selectable="false" space="false" decode="false">
          帮办单位：卫健委
        </text>
        <text class="" selectable="false" space="false" decode="false">
          编号：020501
        </text>
      </view>
    </view>
  </view>
</view>